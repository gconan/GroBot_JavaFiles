<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5" xmlns="http://java.sun.com/xml/ns/javaee">

	<!-- Servlets -->
	
	
	<servlet>
		<servlet-name>greetServlet</servlet-name>
		<servlet-class>utBazaar.server.GreetingServiceImpl</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>greetServlet</servlet-name>
		<url-pattern>/utbazaar/greet</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>registration</servlet-name>
		<servlet-class>utBazaar.servlet.RegistrationServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>registration</servlet-name>
		<url-pattern>/registration</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>resetPassword</servlet-name>
		<servlet-class>utBazaar.servlet.ResetPasswordServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>resetPassword</servlet-name>
		<url-pattern>/resetp</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>deleteItem</servlet-name>
		<servlet-class>utBazaar.servlet.DeleteItemServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>deleteItem</servlet-name>
		<url-pattern>/delete</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>changePassword</servlet-name>
		<servlet-class>utBazaar.servlet.ChangePasswordServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>changePassword</servlet-name>
		<url-pattern>/changep</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>utBazaar.servlet.LoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>utBazaar.servlet.LogoutServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ItemSearch</servlet-name>
		<servlet-class>utBazaar.servlet.SearchServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ItemSearch</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>SellSearch</servlet-name>
		<servlet-class>utBazaar.servlet.SellSearchServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>SellSearch</servlet-name>
		<url-pattern>/sellSearch</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>MeetingSearch</servlet-name>
		<servlet-class>utBazaar.servlet.MeetingSearchServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>MeetingSearch</servlet-name>
		<url-pattern>/meetingSearch</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>Inquiry</servlet-name>
		<servlet-class>utBazaar.servlet.InquireServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Inquiry</servlet-name>
		<url-pattern>/inquire</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>addItem</servlet-name>
		<servlet-class>utBazaar.servlet.AddItemServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>addItem</servlet-name>
		<url-pattern>/addItem</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>addMeeting</servlet-name>
		<servlet-class>utBazaar.servlet.AddMeetingServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>addMeeting</servlet-name>
		<url-pattern>/addMeeting</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>seeAttending</servlet-name>
		<servlet-class>utBazaar.servlet.SeeAttendeesServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>seeAttending</servlet-name>
		<url-pattern>/seeAttending</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>joinMeeting</servlet-name>
		<servlet-class>utBazaar.servlet.JoinMeetingServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>joinMeeting</servlet-name>
		<url-pattern>/goToAMeeting</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ProfilePicture</servlet-name>
		<servlet-class>utBazaar.servlet.AddProfilePictureServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>ProfilePicture</servlet-name>
		<url-pattern>/addProfPic</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>unsubscribe</servlet-name>
		<servlet-class>utBazaar.servlet.UnsubscribeServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>unsubscribe</servlet-name>
		<url-pattern>/unsubscribe</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>email</servlet-name>
		<servlet-class>utBazaar.CronServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>email</servlet-name>
		<url-pattern>/email</url-pattern>
	</servlet-mapping>
	
	<servlet>
    	<servlet-name>DeleteAccount</servlet-name>
		<servlet-class>utBazaar.servlet.DeleteAccountServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>DeleteAccount</servlet-name>
		<url-pattern>/deleteAccount</url-pattern>
	</servlet-mapping>
	
	<filter>
		<filter-name>ObjectifyFilter</filter-name>
		<filter-class>com.googlecode.objectify.ObjectifyFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>ObjectifyFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>

	<!-- Default page to serve -->
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>

</web-app>
